<tr id="domain_competitor_<%= domain_competitor.id %>">
  <td class="px-6 py-4 whitespace-nowrap text-sm text-gray-300" data-sorting-order="desc" data-sorting-item="<%= domain_competitor.serp_appearances_count %>"><%= domain_competitor.competitor_domain %></td>
  <td class="px-6 py-4 whitespace-nowrap text-sm text-gray-300"><%= domain_competitor.serp_appearances_count %></td>
  <td class="px-6 py-4 text-sm text-gray-300">
    <% domain_competitor.search_engine_results.uniq.each do |search_engine_result| %>
      <% keyword = search_engine_result.keyword %>
      <span class="inline-block p-2 mb-2 text-sm font-medium rounded-full <%= search_engine_result.ranking && search_engine_result.ranking <= 10 ? 'bg-green-500/10 text-green-400' : 'bg-yellow-500/10 text-yellow-400' %>">
        <%= link_to "##{search_engine_result.ranking} - #{keyword.name}", search_engine_result.url, class: (search_engine_result.ranking && search_engine_result.ranking <= 10 ? 'text-green-400 hover:text-green-500' : 'text-yellow-400 hover:text-yellow-500') %>
      </span>
    <% end %>
  </td>
</tr>